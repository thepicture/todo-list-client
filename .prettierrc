{
    "tabWidth": 2,
    "useTabs": true,
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "endOfLine": "lf",
    "importOrder": [
        "^app/(.*)$",
        "^processes/(.*)$",
        "^pages/(.*)$",
        "^widgets/(.*)$",
        "^features/(.*)$",
        "^entities/(.*)$",
        "^shared/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}